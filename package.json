{
  "name": "close-tabs-via-regex",
  "displayName": "Close Tabs via Regex",
  "description": "Adds a command that closes all tabs that match a given regex.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [ ],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "close-tabs-via-regex.close-tabs-via-regex",
        "title": "Close Tabs via Regex"
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/index.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "webpack",
    "watch-web": "webpack --watch",
    "package-web": "webpack --mode production --devtool hidden-source-map",
    "lint": "eslint src --ext ts",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/assert": "^1.5.10",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "mocha": "^10.2.0",
    "typescript": "^5.3.3",
    "@vscode/test-web": "^0.0.51",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.0",
    "webpack-cli": "^5.1.4",
    "@types/webpack-env": "^1.18.4",
    "assert": "^2.1.0",
    "process": "^0.11.10"
  }
}
